{% include 'pagehead.html.twig' %}
      <h1>Registrar Dashboard</h1>
      <div class="row">
        <div class="col-md-8">
          {% if courses is not empty %}
          <h2>All Courses</h2>
          <ul>
            {% for course in courses %}
            <li><a href="/new_course/{{ course.getCourseId }}">{{ course.getCourseTitle }}, {{ course.getCourseCode}}</a></li>
            {% endfor %}
          </ul>
          {% endif %}
        </div>
        <div class="col-md-4">
          <h2><a href="/">Home</a></h2>
          <h2><a href="/all_students">View All Students</a></h2>
        </div>
      </div>



