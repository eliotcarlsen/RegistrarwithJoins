{% include 'pagehead.html.twig' %}
      <h1>Registrar Dashboard</h1>
      <div class="row">
        <div class="col-md-8">
          {% if departments is not empty %}
          <h2>All Departments</h2>
          <ul>
            {% for department in departments %}
            <li><a href="/department/{{ department.getDepartmentId }}">{{ department.getDepartmentName }}</a></li>
            {% endfor %}
          </ul>
          {% else %}
          <h2>No Departments Listed</h2>
          {% endif %}
        </div>
        <div class="col-md-4">
          <h2><a href="/">Home</a></h2>
          <h2><a href="/new_course">View All Courses</a></h2>
          <h2><a href="/all_students">View All Students</a></h2>
        </div>
      </div>
{% include 'pagefoot.html.twig' %}
